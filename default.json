{
    "extends": [
      "config:base"
    ],
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": ["^rokit.toml$"],
        "matchStrings": [".+ = \"(?<depName>.+)@(?<currentValue>.+)\""],
        "datasourceTemplate": "github-releases"
      }
    ],
    "packageRules": [
      {
        "matchPaths": ["rokit.toml"],
        "extractVersion": "^v?(?<version>.*)$"
      }
    ]
}
